{
  "name": "@fwdslsh/rabit-client",
  "version": "0.4.0",
  "description": "Reference implementation of Rabit Burrow Traversal (RBT) client - Git-first, agent-friendly content distribution",
  "keywords": [
    "rabit",
    "burrow",
    "warren",
    "traversal",
    "rbt",
    "agent",
    "ai",
    "content",
    "git",
    "manifest"
  ],
  "homepage": "https://github.com/fwdslsh/rabit#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/fwdslsh/rabit.git",
    "directory": "packages/rabit-client"
  },
  "bugs": {
    "url": "https://github.com/fwdslsh/rabit/issues"
  },
  "license": "CC-BY-4.0",
  "author": "Rabit Contributors",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./types": {
      "import": "./dist/types.js",
      "types": "./dist/types.d.ts"
    }
  },
  "bin": {
    "rabit": "./dist/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bun run clean && bun build ./src/index.ts --outdir=dist --target=bun && bun build ./src/cli.ts --outdir=dist --target=bun",
    "build:types": "bun x tsc --emitDeclarationOnly --declaration --outDir dist",
    "dev": "bun --watch src/cli.ts",
    "test": "bun test",
    "test:watch": "bun test --watch",
    "typecheck": "bun x tsc --noEmit",
    "prepublishOnly": "bun run build && bun test",
    "clean": "rm -rf dist"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^25.0.8",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "bun": ">=1.0.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  }
}
